{
  "name": "yumlish",
  "version": "6.1.0",
  "license": "MIT",
  "author": "Webgarh",
  "description": "Angular 2+, Express, Mongoose and Node.",
  "engines": {
    "node": "8.11.1",
    "npm": "6.4.0"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod\" \"ng serve --proxy-config proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build --prod --base-href ./ && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && mocha dist/server/test --exit",
    "lint": "ng lint",
    "linthtml": "htmlhint \"client/**/*.html\"",
    "lintscss": "sass-lint \"client/**/*.scss\" -v",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e",
    "postinstall": "tsc -p server && ng build --aot -prod"
  },
  "private": true,
